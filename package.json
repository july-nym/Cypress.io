{
  "name": "ci",
  "version": "4.4.1",
  "description": "Cypress is a next generation front end testing tool built for the modern web. We address the key pain points developers and QA engineers face when testing modern applications.",
  "main": "index.js",
  "scripts": {
    "10travlr-production": "npx cypress run --spec 'cypress/integration/10travlr/production/regression/**/*' --headless --record --key e73f75bc-9ff2-4600-8eca-1f9570abf53e --parallel --tag '10travlr, production' ",
    "10travlr-regression": "npm run 10travlr-production",
    "10travlr-lokal": "npx cypress run --spec 'cypress/integration/10travlr/production/regression/**/*' --headless --browser chrome",
    "10travlr": "npm run 10travlr-lokal",
    "api-test": "newman run 'cypress/integration/10travlr/production/api-test/10travlr-prod-api-testing.postman_collection.json' ",
    "k6-test": "k6 run '/Users/reposado/Cypress.io/cypress/integration/10travlr/production/k6/smoke-testing.js' ",
    "k6-smoke-testing": "docker run -e K6_CLOUD_TOKEN=$K6_CLOUD_TOKEN -e K6_CLOUD_PROJECT_ID=$K6_CLOUD_PROJECT_ID -v /home/circleci/project:/tmp/Cypress.io -i loadimpact/k6:latest cloud /tmp/Cypress.io/cypress/integration/10travlr/production/k6/smoke-testing.js"

  },
