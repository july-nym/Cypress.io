    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^5.1.0"
  },
  "devDependencies": {
    "cypress": "^4.4.0",
    "cypress-terminal-report": "^1.2.1",
    "mocha": "^7.1.1",
    "mocha-circleci-reporter": "0.0.3"
  }
}
